{% extends "blocks/base/data.json" %}
{% load basic_tags %}

{% block table_rows %}
    {% for ord in records %}
    [

        "{{ord.id|escapejs}}",
        "{{ord|escapejs}}",
        "{{ord.tot_gasmembers|escapejs}}",
        "&#8364; {{ord.tot_price|escapejs|floatformat:2}}",
        "&#8364; {{ord.invoice_amount|escapejs|floatformat:2}}",
        "&#8364; {{ord.tot_curtail|escapejs|floatformat:2}}",
        "{{ord.payment|escapejs|floatformat:2}}",
        "{{ord.urn|escapejs}}",
        "{{ord.payment_urn|escapejs|floatformat:2}}",
        "{{ord.is_canceled}}",

    ]
    {% if not forloop.last %}
    ,
    {% endif %}
    {% endfor %}

{% endblock %}
