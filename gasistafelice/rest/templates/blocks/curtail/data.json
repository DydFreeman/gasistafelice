{% extends "blocks/base/edit_multiple.json" %}
{% load basic_tags %}

{% block table_rows %}
    {% for row in records %}
    [

        "{{row.order_id|escapejs}}",
        "{{row.purchaser_id|escapejs}} - {{row.gasmember.upper|escapejs}}",
        "&#8364; {{row.sum_amount|floatformat:"2"}}",
        "{{row.amounted|escapejs}}",

    ]
    {% if not forloop.last %}
        ,
    {% endif %}
    {% endfor %}

{% endblock %}

