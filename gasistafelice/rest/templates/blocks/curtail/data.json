{% extends "blocks/base/edit_multiple.json" %}
{% load basic_tags %}

{% block table_rows %}
    {% for row in records %}
    [

        "{{row.pk|escapejs}}",
        "{{row|escapejs}}",
        "&#8364; {{row.sum_amount|floatformat:"2"}}",
        "{{row.accounted_amount|floatformat:"2"}}",
        "{% filter escapejs %}{% bool_img row.accounted_amount %}{% endfilter %}",

    ]
    {% if not forloop.last %}
        ,
    {% endif %}
    {% endfor %}

{% endblock %}

