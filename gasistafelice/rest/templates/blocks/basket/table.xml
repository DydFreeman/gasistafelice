{% extends "blocks/stocks/table.xml" %}
{% load i18n %}

{% block headers %}
    <th>{% trans "Id" %}</th>
    <th>{% trans "Order" %}</th>
    <th>{% trans "Supplier" %}</th>
    <th>{% trans "Product" %}</th>
    <th>{% trans "Price" %}</th>
    <th>{% trans "Ordered amount" %}</th>
    <th>{% trans "Ordered total" %}</th>
    <th>{% trans "Delete" %}</th>
{% endblock %}

{% block footer %}
    <tr>
        <th class="totfield" colspan="5">{% trans "Total" %}:</th>
        <th class="totvalue" colspan="3" id="total-order"></th>
    </tr>
{% endblock %}

