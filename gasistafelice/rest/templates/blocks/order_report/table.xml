{% extends "blocks/stocks/table.xml" %}
{% load i18n %}

{% block headers %}
    <th>{% trans "Product" %}</th>
    <th>{% trans "Price" %}</th>
    <th>{% trans "TOT GAS members" %}</th>
    <th>{% trans "TOT amount" %}</th>
    <th>{% trans "TOT price" %}</th>
    <th>{% trans "Available" %}</th>
{% endblock %}

{% block footer %}
    <tr><th colspan="4" style="text-align:right" rowspan="1">{% trans "Total" %}:</th><th rowspan="1" colspan="1" id="total-order"> </th></tr>
{% endblock %}
