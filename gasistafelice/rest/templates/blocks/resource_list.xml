
<block type="{{ block_type }}" 
       resource_type="{{ resource.resource_type }}" 
       resource_id="{{ resource.id }}">
       
	<content type="list">
	
		{% for res in resource_list %}
            {% block display_resource_entry %}
			<info type="{{ res.resource_type }}" sanet_urn="{{ res.urn }}" name="{{res}}" />
            {% endblock %}
		{% endfor %} 
		
	</content>
    
	<content type="user_actions">
		{% for a in user_actions %}
		<action verbose_name="{{ a.verbose_name }}" name="{{ a.name }}" url="{{ a.url }}" has_form="{{ a.has_form|yesno:"1,0" }}" />
		{% endfor %}
	</content>

</block>

