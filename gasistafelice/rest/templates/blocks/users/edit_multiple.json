{% extends "blocks/base/edit_multiple.json" %}
{% load basic_tags %}

{% block table_rows %}
    {% for ss in records %}
    [

        "{{ss.id|escapejs}}",
        "{{ss.username|escapejs}}",
        "{{ss.first_name|escapejs}}",
        "{{ss.last_name|escapejs}}",
        "{{ss.email|escapejs}}",
        "{{ss.last_login|date:"Y b d H:i"}}",
        "{{ss.date_joined|date:"Y b d H:i"}}",
        "{% filter escapejs %}{% bool_img ss.is_staff %}{% endfilter %}",
        "{{ss.is_active|escapejs}}",

    ]
    {% if not forloop.last %}
    ,
    {% endif %}
    {% endfor %}

{% endblock %}

