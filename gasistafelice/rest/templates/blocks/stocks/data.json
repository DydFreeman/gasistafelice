{% extends "blocks/base/data.json" %}
{% load basic_tags %}

{% block table_rows %}
    {% for ss in records %}
    [

        "{{ss.id|escapejs}}",
        "{{ss.product|escapejs}}",
        "{{ss.product.description|escapejs}}",
        "&#8364; {{ss.price|floatformat:2|escapejs}}",
        "{% filter escapejs %}{% bool_img ss.availability %}{% endfilter %}",
        "{{ss.urn|escapejs}}",

    ]
    {% if not forloop.last %}
    ,
    {% endif %}
    {% endfor %}

{% endblock %}
