{% extends "blocks/base/edit_multiple.json" %}

{% block table_rows %}
    {% for stock in records %}
    [

        "{{stock.id|escapejs}}",
        "{{stock.code|escapejs}}",
        "{{stock.product|escapejs}}",
        "{{stock.product.description|escapejs}}",
        "&#8364; {{ stock.price|escapejs }}",
        "{% filter escapejs %}{{ stock.availability }}{% endfilter %}",

    ]
    {% if not forloop.last %}
    ,
    {% endif %}
    {% endfor %}

{% endblock %}

