{% extends "blocks/base/edit_multiple.json" %}
{% load basic_tags %}

{% block table_rows %}
    {% for ss in records %}
    [

        "{{ss.id|escapejs}}",
        "{{ss.product|escapejs}}",
        "{{ss.category|truncatetostr:'::'|escapejs}}",
