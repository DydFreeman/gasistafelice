{% load basic_tags %}
{
    "sEcho": {{sEcho}},
    "iTotalRecords": {{iTotalRecords}},
    "iTotalDisplayRecords": {{iTotalDisplayRecords}},
    "aaData":[
    {% for stock in records %}
    [
        
        "{{stock.id|escapejs}}",
        "{{stock.code|escapejs}}",
        "{{stock.product|escapejs}}",
        "{{stock.description|escapejs}}",
        "&#8364; {{ stock.price|escapejs }}",
        "{% filter escapejs %}{{ stock.availability }}{% endfilter %}",
    ]
    {% if not forloop.last %}
    ,
    {% endif %}
    {% endfor %}
    ],
    "gf_moreData": { {{ moreData }} }
}
