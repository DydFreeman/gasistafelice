{% extends "blocks/base/edit_multiple.json" %}
{% load basic_tags %}

{% block table_rows %}
    {% for gss in records %}
    [

        "{{gss.id|escapejs}}",
        "{{gss.product|escapejs}}",
        "&#8364; {{gss.price|floatformat:2 }}",
        "{% filter escapejs %}{% bool_img gss.availability %}{% endfilter %}",
        "{{gss.field_enabled|escapejs}}",
        "{{gss.tot_amount|escapejs}}",
        "{{gss.tot_gasmembers|escapejs}}",
        "{{gss.tot_price|escapejs}}",

    ]
    {% if not forloop.last %}
    ,
    {% endif %}
    {% endfor %}

{% endblock %}

