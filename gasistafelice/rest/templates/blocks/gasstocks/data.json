{% extends "blocks/base/data.json" %}
{% load basic_tags %}

{% block table_rows %}
    {% for gss in records %}
    [

        "{{gss.id|escapejs}}",
        "{{gss.stock.product|escapejs}}",
        "{{gss.stock.product.description|escapejs}}",
        "&#8364; {{ gss.price|floatformat:2 }}",
        "{% filter escapejs %}{% bool_img gss.stock.availability %}{% endfilter %}",
        "{% filter escapejs %}{% bool_img gss.enabled %}{% endfilter %}",

    ]
    {% if not forloop.last %}
    ,
    {% endif %}
    {% endfor %}

{% endblock %}
