{% extends "blocks/base/data.json" %}
{% load basic_tags %}

{% block table_rows %}
    {% for stock in records %}
    [

        "{{stock.id|escapejs}}",
        "{{stock.product|escapejs}}",
        "{{stock.product.description|escapejs}}",
        "&#8364; {{ stock.price|floatformat:2 }}",
        "{% filter escapejs %}{% bool_img stock.availability %}{% endfilter %}",
        "{% filter escapejs %}{% bool_img stock.field_enabled %}{% endfilter %}",

    ]
    {% if not forloop.last %}
    ,
    {% endif %}
    {% endfor %}

{% endblock %}
