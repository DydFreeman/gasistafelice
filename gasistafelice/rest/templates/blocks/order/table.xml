{% extends "blocks/base/table.xml" %}
{% load i18n %}

{% block headers %}
    <th>{% trans "Id" %}</th>
    <th>{% trans "Supplier" %}</th>
    <th>{% trans "Product" %}</th>
    <th>{% trans "Description" %}</th>
    <th>{% trans "Price" %}</th>
    <th>{% trans "Ordered amount" %}</th>
    <th>{% trans "Ordered total" %}</th>
{% endblock %}

{% block footer %}
    <tr><th colspan="4" style="text-align:right" rowspan="1">{% trans "Total" %}:</th><th rowspan="1" colspan="3" id="total-order"> </th></tr>
{% endblock %}

