{% extends "blocks/base/data.json" %}
{% load basic_tags %}

{% block table_rows %}
    {% for entry in records %}
    [

        "{{entry.pk|escapejs}}",
        "{{entry.date|escapejs}}",
        "{{entry.account.name|escapejs}}",
        "{{entry.transaction.kind|escapejs}}",
        "{{entry.description|escapejs}}",
        "{{entry.amount|escapejs}}",

    ]
    {% if not forloop.last %}
    ,
    {% endif %}
    {% endfor %}

{% endblock %}
