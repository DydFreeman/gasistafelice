{% extends "blocks/base/data.json" %}
{% load basic_tags %}

{% block table_rows %}
    {% for entry in records %}
    [

        "{{entry.pk|escapejs}}",
        "{{entry.date|escapejs}}",
        "{% filter escapejs %}{% Human_readable_account entry.account %}{% endfilter %}",
        "{{entry.transaction.kind|escapejs}}",
        "{{entry.amount|escapejs}}",
        "{{entry.description|escapejs}}",
    ]
    {% if not forloop.last %}
    ,
    {% endif %}
    {% endfor %}

{% endblock %}
